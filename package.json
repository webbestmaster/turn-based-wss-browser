  "dependencies": {
    "lodash": "^4.17.10",
    "sha1": "^1.1.1",
    "timer-stopwatch": "^0.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "chai": "^4.1.2",
    "chai-json-schema": "^1.5.0",
    "eslint": "^4.19.1",
    "eslint-plugin-flowtype": "^2.46.3",
    "flow-bin": "^0.72.0",
    "flow-remove-types": "^1.2.3",
